separate(urls,
into = c("bin", "bin2", "bin3", "year", "bin4", "bin5", "episode"),
sep = "\\/",
remove = FALSE) %>%
mutate(urls = as.character(urls), episode = str_replace_all(episode, "\\-.*", "")) %>%
select(urls, year, episode)
urls
}
